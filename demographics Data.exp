<radiobuttons sex>
/ caption = "Sex"
/ options = ("Female", "Male")
</radiobuttons>
*********************************************************************************************************************************************************************************
<textbox age>
/ caption = "Age"
/ mask = decimal
/ range = (7, 110)
</textbox>
**********************************************************************************************************************************************************************************
<dropdown study>
/ caption = " What are you currently studying"
/ options = ("Applied Mathematics","Applied Physics","Biomedical Engineering","Business Administration","Business Information Technology",
"Chemical Engineering","Civil Engineering and Management","Communication Studies","Computer Science","Construction Management and Engineering*",
"Educational Science and Technology","Electrical Engineering","Embedded Systems*","European Studies","Health Sciences",
"Human Media Interaction","Industrial Engineering and Management","Industrial Design Engineering","Leraar VHO Maatschappijleer**",
"Mechanical Engineering","Nanotechnology","Philosophy of Science, Technology and Society","Psychology","Public Administration",
"Science Education and Communication","Social Systems Evaluation and Survey Research","Sustainable Energy Technology")
</dropdown>
**********************************************************************************************************************************************************************************
<radiobuttons pcownage>
/ caption = "Do you own a Personal Computer"
/ options = ("Yes", "No")
</radiobuttons>
**********************************************************************************************************************************************************************************
<checkboxes consoles>
/ caption = "Which of the following do you use to play most of your video games on? (you can check multiple boxes if you want to)"
/ options = ("PC","xBox 360", "Nintendo WII", "Sony Playstation", "Portable devices such as  Mobile Phone, Nintendo ds or PSP","Microsoft Kinnect","Emotive EPOC/OCZ nia/Neurosky Mindwave Headset", "None")
</checkboxes>
**********************************************************************************************************************************************************************************
<textbox dailypcusage>
/ caption = "How many hours do you use your computer daily? (example answer: 1.5 )"
/ mask = decimal
/ range = (0, 20)
</textbox>
**********************************************************************************************************************************************************************************
<checkboxes gametypes>
/ caption = "What kind of games do you like to play? (you can check multiple boxes if you want to)"
/ options = ("First Person Shooter", "Third Person Shooter", "Massively Multiplayer Online Game ","Racing Games","Sport Games", "Others", "I dont like games at all")
</checkboxes>
**********************************************************************************************************************************************************************************
<textbox weeklygameusage>
/ caption = "How many hours do you spend weekly playing video games(example answer: 1.5 )  "
/ mask = decimal
/ range = (0, 120)
</textbox>
**********************************************************************************************************************************************************************************
<radiobuttons playgames>
