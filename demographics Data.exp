<slider arousal>
/ caption = "How arousing or relaxing was the music when you were playing the video game?"
/ labels = ("very~narousing", "moderately~narousing", "slightly~narousing", "no~nopinion", "slightly~nrelaxing", "moderately~nrelaxing", "very~nrelaxing")
/ range = (1, 7)
/ slidersize = (60%, 5%)
/ showtooltips = false
</slider>

<slider interesting>
/ caption = "How interesting or boring was the music when you were playing the video game?"
/ labels = ("very~ninteresting", "moderatly~ninteresting ", "slightly~ninteresting ", "no~nopinion", "slightly~nboring ", "moderalty~nboring ", "very~nboring")
/ range = (1, 7)
/ slidersize = (60%, 5%)
